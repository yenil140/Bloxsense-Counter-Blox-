table.insert(cors,sandbox(LocalScript1298,function()
local button = script.Parent
local toggled = false
local plrs = game:GetService("Players")
local plr = plrs.LocalPlayer

local SELFCHAMTHREAD = coroutine.create(function()
	while wait() do
		if script.SELFCOLOR.Value == false then
			script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUCOLOR.Value
		else
			script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUTOGGLEDCOLOR.Value					
		end
	end
end)

local SELFCHAMTHREAD1 = coroutine.create(function()
	while wait(1/100) do
		if script.SELFCOLOR.Value == true then
			local Trans = script.Parent.Parent.LocalTrans.Percent.Value/100
			local Color = script.Parent.Color.BackgroundColor3
			if plr.Character then
				if script.Parent.Parent.DropDown2.Selection.Text == ("Glass") then
					for _,v in pairs(plrs.LocalPlayer.Character:GetChildren()) do 
						if v:IsA("MeshPart") or v:IsA("BasePart") and v.Name ~= "HumanoidRootPart" and v.Name ~= "HeadHB" and v.Name ~= "Gun" then
							pcall(function() 
								v.Material = Enum.Material.Glass
								v.Color = Color
								v.Transparency = Trans
								v.TextureId = ""
							end)
						end
					end
				end

				if script.Parent.Parent.DropDown2.Selection.Text == ("Forcefield") then
					for _,v in pairs(plrs.LocalPlayer.Character:GetChildren()) do 
						if v:IsA("MeshPart") or v:IsA("BasePart") and v.Name ~= "HumanoidRootPart" and v.Name ~= "HeadHB" and v.Name ~= "Gun" then
							pcall(function() 
								v.Material = Enum.Material.ForceField
								v.Color = Color
								v.Transparency = Trans
							end)
						end
					end
				end
				if script.Parent.Parent.DropDown2.Selection.Text == ("Pulse") then
					for _,v in pairs(plrs.LocalPlayer.Character:GetChildren()) do 
						if v:IsA("MeshPart") or v:IsA("BasePart") and v.Name ~= "HumanoidRootPart" then
							pcall(function() 
								v.Material = Enum.Material.SmoothPlastic
								v.Color = Color

								local tweenService = game:GetService("TweenService")
								local tweenInfo = TweenInfo.new(
									0.4, --Time
									Enum.EasingStyle.Sine, --EasingStyle
									Enum.EasingDirection.Out, --EasingDirection
									0, --Repeat count
									false, --Reverses if true
									0 --Delay time
								)
								local tweenA = tweenService:Create(plrs.LocalPlayer.Character.LeftFoot, tweenInfo, {Transparency = 1})
								local tweenA2 = tweenService:Create(plrs.LocalPlayer.Character.LeftFoot, tweenInfo, {Transparency = 0})

								local tweenB = tweenService:Create(plrs.LocalPlayer.Character.LeftHand, tweenInfo, {Transparency = 1})
								local tweenB2 = tweenService:Create(plrs.LocalPlayer.Character.LeftHand, tweenInfo, {Transparency = 0})

								local tweenC = tweenService:Create(plrs.LocalPlayer.Character.LeftLowerArm, tweenInfo, {Transparency = 1})
								local tweenC2 = tweenService:Create(plrs.LocalPlayer.Character.LeftLowerArm, tweenInfo, {Transparency = 0})

								local tweenD = tweenService:Create(plrs.LocalPlayer.Character.LeftLowerLeg, tweenInfo, {Transparency = 1})
								local tweenD2 = tweenService:Create(plrs.LocalPlayer.Character.LeftLowerLeg, tweenInfo, {Transparency = 0})

								local tweenE = tweenService:Create(plrs.LocalPlayer.Character.LowerTorso, tweenInfo, {Transparency = 1})
								local tweenE2 = tweenService:Create(plrs.LocalPlayer.Character.LowerTorso, tweenInfo, {Transparency = 0})

								local tweenF = tweenService:Create(plrs.LocalPlayer.Character.RightFoot, tweenInfo, {Transparency = 1})
								local tweenF2 = tweenService:Create(plrs.LocalPlayer.Character.RightFoot, tweenInfo, {Transparency = 0})

								local tweenG = tweenService:Create(plrs.LocalPlayer.Character.RightHand, tweenInfo, {Transparency = 1})
								local tweenG2 = tweenService:Create(plrs.LocalPlayer.Character.RightHand, tweenInfo, {Transparency = 0})

								local tweenH = tweenService:Create(plrs.LocalPlayer.Character.RightLowerArm, tweenInfo, {Transparency = 1})
								local tweenH2 = tweenService:Create(plrs.LocalPlayer.Character.RightLowerArm, tweenInfo, {Transparency = 0})

								local tweenI = tweenService:Create(plrs.LocalPlayer.Character.RightLowerLeg, tweenInfo, {Transparency = 1})
								local tweenI2 = tweenService:Create(plrs.LocalPlayer.Character.RightLowerLeg, tweenInfo, {Transparency = 0})

								local tweenJ = tweenService:Create(plrs.LocalPlayer.Character.RightUpperArm, tweenInfo, {Transparency = 1})
								local tweenJ2 = tweenService:Create(plrs.LocalPlayer.Character.RightUpperArm, tweenInfo, {Transparency = 0})

								local tweenK = tweenService:Create(plrs.LocalPlayer.Character.RightUpperLeg, tweenInfo, {Transparency = 1})
								local tweenK2 = tweenService:Create(plrs.LocalPlayer.Character.RightUpperLeg, tweenInfo, {Transparency = 0})

								local tweenL = tweenService:Create(plrs.LocalPlayer.Character.UpperTorso, tweenInfo, {Transparency = 1})
								local tweenL2 = tweenService:Create(plrs.LocalPlayer.Character.UpperTorso, tweenInfo, {Transparency = 0})

								local tweenM = tweenService:Create(plrs.LocalPlayer.Character.LeftUpperArm, tweenInfo, {Transparency = 1})
								local tweenM2 = tweenService:Create(plrs.LocalPlayer.Character.LeftUpperArm, tweenInfo, {Transparency = 0})

								local tweenN = tweenService:Create(plrs.LocalPlayer.Character.LeftUpperLeg, tweenInfo, {Transparency = 1})
								local tweenN2 = tweenService:Create(plrs.LocalPlayer.Character.LeftUpperLeg, tweenInfo, {Transparency = 0})

								local tweenO = tweenService:Create(plrs.LocalPlayer.Character.Head, tweenInfo, {Transparency = 1})
								local tweenO2 = tweenService:Create(plrs.LocalPlayer.Character.Head, tweenInfo, {Transparency = 0})

								while wait(0.5) do
									tweenA:Play()
									tweenB:Play()
									tweenC:Play()
									tweenD:Play()
									tweenE:Play()
									tweenF:Play()
									tweenG:Play()
									tweenH:Play()
									tweenI:Play()
									tweenJ:Play()
									tweenK:Play()
									tweenL:Play()
									tweenM:Play()
									tweenN:Play()
									tweenO:Play()
									wait(0.4)	
									tweenA2:Play()
									tweenB2:Play()
									tweenC2:Play()
									tweenD2:Play()
									tweenE2:Play()
									tweenF2:Play()
									tweenG2:Play()
									tweenH2:Play()
									tweenI2:Play()
									tweenJ2:Play()
									tweenK2:Play()
									tweenL2:Play()
									tweenM2:Play()
									tweenN2:Play()
									tweenO2:Play()
								end
							end)
						end
					end
				end

				if script.Parent.Parent.DropDown2.Selection.Text == ("Plastic") then
					for _,v in pairs(plrs.LocalPlayer.Character:GetChildren()) do 
						if v:IsA("MeshPart") or v:IsA("BasePart") and v.Name ~= "HumanoidRootPart" and v.Name ~= "HeadHB" and v.Name ~= "Gun" then
							pcall(function() 
								v.Material = Enum.Material.SmoothPlastic
								v.Color = Color
								v.Transparency = 0
								v.Reflectance = 0
								v.TextureId = ""
							end)
						end
					end
				end
			end
		end
	end
end)

coroutine.resume(SELFCHAMTHREAD)
coroutine.resume(SELFCHAMTHREAD1)

local function onButtonActivated()
	if script.SELFCOLOR.Value == false then
		script.SELFCOLOR.Value = true
	else
		script.SELFCOLOR.Value = false
	end		
end	
