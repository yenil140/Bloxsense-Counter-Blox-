				IMG.ImageColor3 = Color3.fromRGB(208, 208, 208)
			end
		end
		button.Activated:Connect(onButtonActivated)
		IMG.Activated:Connect(onButtonActivated1)
	end
	
end
local function onMouseLeft()
	if script.Parent.Main.Visible == false then
		IMG.ImageColor3 = Color3.fromRGB(116, 116, 116)
	end
end

     
	
button.MouseEnter:Connect(onMouseEntered)
button.MouseLeave:Connect(onMouseLeft)


end))
LocalScript1389.Name = "setup"
LocalScript1389.Parent = ScreenGui0
table.insert(cors,sandbox(LocalScript1389,function()
local valid_chars = {}

local function set_valid(x, y)
	for i = string.byte(x), string.byte(y) do
		table.insert(valid_chars, string.char(i))
	end
end

local function random_string(length)
	local s = {}
	for i = 1, length do s[i] = valid_chars[math.random(1, #valid_chars)] end
	return table.concat(s)
end

set_valid('a', 'z')
set_valid('A', 'Z')
set_valid('0', '9')

spawn(function()
	game["Run Service"].RenderStepped:Connect(function()
		script.Parent.Name = "[$$$] "..random_string(4).."_"..random_string(4).."_"..random_string(4).."_".." [$$$]"	
	end)
end)

