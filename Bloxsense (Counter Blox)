LocalScript932.Name = "Toggle"
LocalScript932.Parent = ImageButton931
table.insert(cors,sandbox(LocalScript932,function()
local button = script.Parent
local plrs = game:GetService("Players")
local plr = plrs.LocalPlayer
local CurrentCamera = game.Workspace.CurrentCamera
local runserv = game:GetService("RunService")

local SpecAA = coroutine.create(function()
	runserv.Stepped:Connect(function()
		if script.ANTIAIMBOT.Value == true then
			pcall(function()
				local ReplicateCamera = game.ReplicatedStorage.Events.ReplicateCamera
				plr.CameraCF.Value = plr.Character.Head.CFrame
				ReplicateCamera:FireServer(plr.Character.Head.CFrame)	
			end)
		end
	end)
end)
	
coroutine.resume(SpecAA)

spawn(function()
	while true do
		wait()
		if script.ANTIAIMBOT.Value == false then
			script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUCOLOR.Value
		else
			script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUTOGGLEDCOLOR.Value	
		end

	end
end)

local function onButtonActivated()
	if script.ANTIAIMBOT.Value == false then
		script.ANTIAIMBOT.Value = true
	else
		script.ANTIAIMBOT.Value = false		
	end
end

button.Activated:Connect(onButtonActivated)



end))
