TextLabel86.Parent = Frame76
TextLabel86.Position = UDim2.new(0, 0, 0, -122)
TextLabel86.Size = UDim2.new(0, 324, 0, 20)
TextLabel86.BackgroundColor = BrickColor.new("Institutional white")
