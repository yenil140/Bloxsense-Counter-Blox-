LocalScript1325.Name = "Viewmodel"
LocalScript1325.Parent = ImageButton1323
table.insert(cors,sandbox(LocalScript1325,function()
local button = script.Parent
local TP = script.Parent.Parent.Parent.EffectsTab.ThirdPerson

local GUNSThread = coroutine.create(function()
	game.Workspace.CurrentCamera.ChildAdded:Connect(function(ViewmodelFolder)
		local WeaponTransparency = script.Parent.Parent.WeaponTrans.Percent.Value/100
		if ViewmodelFolder.Name == 'Arms' and script.GUNCOLOR.Value == true and script.Parent.Parent.DropDown3.Selection.Text == "Forcefield" and TP.TP.Value == false then
			local ArmsColorsWeaponsColor = script.Parent.Color.BackgroundColor3
			for i,v in pairs(ViewmodelFolder:GetChildren()) do
				if v:IsA("Model") and v:FindFirstChild("Left Arm") and v:FindFirstChild("Right Arm") then
					-- Arms Pointer
					local RightArm = v["Right Arm"]
					local LeftArm = v["Left Arm"]
					-- Gloves Pointer
					local RightGlove = RightArm:FindFirstChild("Glove") or RightArm:FindFirstChild("RGlove")
					local LeftGlove = LeftArm:FindFirstChild("Glove") or LeftArm:FindFirstChild("LGlove")
					-- Sleeves Pointer
					local RightSleeve = RightArm:FindFirstChild("Sleeve") or nil
					local LeftSleeve = LeftArm:FindFirstChild("Sleeve") or nil

					if RightSleeve and LeftSleeve then

					end
				end
				if v:IsA("BasePart") and v.Name ~= ("Right Arm" or "Left Arm" or "Flash") and v.Transparency~=1 then -- Weapons Pointer
					local Weapons = v

					Weapons.Transparency = WeaponTransparency
					Weapons.Reflectance = 0
					Weapons.Color = ArmsColorsWeaponsColor
					Weapons.Material = "ForceField"

				end
			end
		end
		if ViewmodelFolder.Name == 'Arms' and script.GUNCOLOR.Value == true and script.Parent.Parent.DropDown3.Selection.Text == "Glass" and TP.TP.Value == false then
			local ArmsColorsWeaponsColor = script.Parent.Color.BackgroundColor3
			for i,v in pairs(ViewmodelFolder:GetChildren()) do
				if v:IsA("Model") and v:FindFirstChild("Left Arm") and v:FindFirstChild("Right Arm") then
					-- Arms Pointer
					local RightArm = v["Right Arm"]
					local LeftArm = v["Left Arm"]
					-- Gloves Pointer
					local RightGlove = RightArm:FindFirstChild("Glove") or RightArm:FindFirstChild("RGlove")
					local LeftGlove = LeftArm:FindFirstChild("Glove") or LeftArm:FindFirstChild("LGlove")
					-- Sleeves Pointer
					local RightSleeve = RightArm:FindFirstChild("Sleeve") or nil
					local LeftSleeve = LeftArm:FindFirstChild("Sleeve") or nil


					if RightSleeve and LeftSleeve then


					end
				end
				if v:IsA("BasePart") and v.Name ~= ("Right Arm" or "Left Arm" or "Flash") and v.Transparency~=1 then -- Weapons Pointer
					local Weapons = v

					Weapons.Transparency = WeaponTransparency
					Weapons.Reflectance = 0
					if Weapons.TextureID then
						Weapons.TextureID = ""	
					end
					Weapons.Color = ArmsColorsWeaponsColor
					Weapons.Material = "Neon"

				end
			end
		end
		if ViewmodelFolder.Name == 'Arms' and script.GUNCOLOR.Value == true and script.Parent.Parent.DropDown3.Selection.Text == "Plastic" and TP.TP.Value == false then
			local ArmsColorsWeaponsColor = script.Parent.Color.BackgroundColor3
			for i,v in pairs(ViewmodelFolder:GetChildren()) do
				if v:IsA("Model") and v:FindFirstChild("Left Arm") and v:FindFirstChild("Right Arm") then
					-- Arms Pointer
					local RightArm = v["Right Arm"]
					local LeftArm = v["Left Arm"]
					-- Gloves Pointer
					local RightGlove = RightArm:FindFirstChild("Glove") or RightArm:FindFirstChild("RGlove")
					local LeftGlove = LeftArm:FindFirstChild("Glove") or LeftArm:FindFirstChild("LGlove")
					-- Sleeves Pointer
					local RightSleeve = RightArm:FindFirstChild("Sleeve") or nil
					local LeftSleeve = LeftArm:FindFirstChild("Sleeve") or nil

				end
				if v:IsA("BasePart") and v.Name ~= ("Right Arm" or "Left Arm" or "Flash") and v.Transparency~=1 then -- Weapons Pointer
					local Weapons = v

					Weapons.Transparency = WeaponTransparency
					Weapons.Reflectance = 0
					if Weapons.TextureID then
						Weapons.TextureID = ""	
					end
					Weapons.Color = ArmsColorsWeaponsColor
					Weapons.Material = "SmoothPlastic"

				end
			end
		end		
	end)
end)

local GUNSThread2 = coroutine.create(function()
	while wait() do
		if script.GUNCOLOR.Value == false then
			script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUCOLOR.Value
		else
			script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUTOGGLEDCOLOR.Value
		end
	end
end)

coroutine.resume(GUNSThread)
coroutine.resume(GUNSThread2)

local function onButtonActivated()
	if script.GUNCOLOR.Value == false then
		script.GUNCOLOR.Value = true		
	else
		script.GUNCOLOR.Value = false
	end
end
button.Activated:Connect(onButtonActivated)
end))
