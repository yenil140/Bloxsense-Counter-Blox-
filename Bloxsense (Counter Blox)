local plrs = game:GetService("Players")
local plr = plrs.LocalPlayer
local button = script.Parent
local toggled = false

local players=game:GetService("Players")
local currentPlayer=nil
local lplayer=players.LocalPlayer

local espFolder2 = Instance.new("Folder")
syn.protect_gui(espFolder2)
espFolder2.Parent = game.CoreGui
espFolder2.Name = "inner"
local TOGGLED = false
local pulsetrans = 0

local pulsestep = coroutine.create(function()
	while wait() do
		for i = 0, 2, 0.02 do
			game["Run Service"].RenderStepped:wait()
			pulsetrans = math.abs(-(i-1))
		end	
	end	
end)

local ICHAMTOGGLECOLORS = coroutine.create(function()
	while wait() do
		if script.INNERCHAMS.Value == false then
			script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUCOLOR.Value
		else
			script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUTOGGLEDCOLOR.Value
			local color = script.Parent.Color.BackgroundColor3
			for i,v in next,espFolder2:GetChildren() do
				if v:IsA("BoxHandleAdornment") then
					v.Color3 = color
					if script.Parent.Parent.DropDown4.Selection.Text == "Pulse" then
						v.Transparency = pulsetrans
					end
					if script.Parent.Parent.DropDown4.Selection.Text == "Custom" then
						v.Transparency = script.Parent.Parent.ChamTrans.Percent.Value/100
					end
				end
			end
		end
	end	
end)

local ThreadICham = coroutine.create(function()
	script.INNERCHAMS.Changed:Connect(function()
		if script.INNERCHAMS.Value == true then
			for i,v in next, game:GetService'Players':GetChildren() do
				if v~=plr then
					if workspace:FindFirstChild(v.Name) then
						if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
							if script.Parent.Parent.Parent.PlayerESPTab.Teammates.teammates.TEAMMATEESP.Value == true then
								wait()
								esp(v)
							end
						else
							wait()
							esp(v)
						end
					end
				end
			end
		else
			for	i,v in pairs(game.CoreGui.inner:GetDescendants())  do
				if v:IsA("BoxHandleAdornment") then
					v:Destroy()
				end
			end
		end
	end)
end)

function esp(plr)
	pcall(function()
		if plr.Character then
			for i,v in next,plr.Character:GetChildren() do
				if v:IsA("BasePart") and v.Name ~= "HumanoidRootPart" and v.Name ~= "Gun" then
					local chamThing = Instance.new("BoxHandleAdornment",espFolder2)
					chamThing.Size = v.Size
					chamThing.Transparency = 0
					chamThing.ZIndex = 2
					chamThing.AlwaysOnTop = true
					chamThing.Visible = true
					chamThing.Adornee = v
					plr.Character.HumanoidRootPart.AncestryChanged:connect(function()
						chamThing:Destroy()
					end)
				end
			end
		end	
	end)
end

local ThreadIChamA = coroutine.create(function()
	for i,v in pairs(game.Players:GetChildren()) do
		v.CharacterAdded:connect(function()
			if v~=plr then
				if script.INNERCHAMS.Value == true then
					v.Character:WaitForChild('Head')
					if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
						if script.Parent.Parent.Parent.PlayerESPTab.Teammates.teammates.TEAMMATEESP.Value == true then
							wait()
							esp(v)
						end
					else
						wait()
						esp(v)
					end
				end
			end
		end)
	end
end)

local ThreadIChamB = coroutine.create(function()
	game.Players.PlayerAdded:connect(function(v)
		v.CharacterAdded:connect(function()
			if v~=plr then
				if script.INNERCHAMS.Value == true then
					v.Character:WaitForChild('Head')
					if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
						if script.Parent.Parent.Parent.PlayerESPTab.Teammates.teammates.TEAMMATEESP.Value == true then
							wait()
							esp(v)
						end
					else
						wait()
						esp(v)
					end
				end
			end
		end)
	end)	
end)

coroutine.resume(ThreadICham)
coroutine.resume(ThreadIChamA)
coroutine.resume(ThreadIChamB)
coroutine.resume(ICHAMTOGGLECOLORS)
coroutine.resume(pulsestep)

local button = script.Parent
local function onButtonActivated()
	if script.INNERCHAMS.Value == false then
		script.INNERCHAMS.Value = true
	else
		script.INNERCHAMS.Value = false
	end
end

button.Activated:Connect(onButtonActivated)

end))
