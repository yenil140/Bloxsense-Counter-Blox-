table.insert(cors,sandbox(LocalScript1255,function()
local plrs = game:GetService("Players")
local plr = plrs.LocalPlayer
local toggled = false
local espFolder3 = Instance.new("Folder")
syn.protect_gui(espFolder3)
espFolder3.Parent = game.CoreGui
espFolder3.Name = "bones"
local thicccness = 0.02

local BONESCOLOR_THREAD = coroutine.create(function()
	while true do
		wait()
		local color = script.Parent.Color.BackgroundColor3
		for	i,v in pairs(game.CoreGui.bones:GetDescendants()) do
			if v:IsA("BoxHandleAdornment") then
				v.Color3 = color
			end
		end
	end
end)

local BONESTHREAD = coroutine.create(function()
	while true do
		wait()
		if script.SKELETONESP.Value == false then
			script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUCOLOR.Value
		else
			script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUTOGGLEDCOLOR.Value
		end
	end
end)

function esp(plr)
	if plr.Character then
		for i,v in next,plr.Character:GetChildren() do
			if v:IsA("BasePart") and v.Name ~= "HumanoidRootPart" and v.Name ~= "Gun" then
				local transparency = 0
				local BoxHandleAdornment0 = Instance.new("BoxHandleAdornment")
				BoxHandleAdornment0.Name = "LeftLowerArm"
				BoxHandleAdornment0.Parent = espFolder3
				BoxHandleAdornment0.Color = BrickColor.new("Institutional white")
				BoxHandleAdornment0.Transparency = transparency
				BoxHandleAdornment0.Size = Vector3.new(thicccness, 0.699999988, thicccness)
				BoxHandleAdornment0.Color3 = Color3.new(1, 1, 1)
				BoxHandleAdornment0.ZIndex = 10
				BoxHandleAdornment0.Adornee = v.Parent.LeftLowerArm
				BoxHandleAdornment0.AlwaysOnTop = true
				BoxHandleAdornment0.SizeRelativeOffset = Vector3.new(0, -0.25, 0)

				local BoxHandleAdornment1 = Instance.new("BoxHandleAdornment")
				BoxHandleAdornment1.Name = "LeftLowerLeg"
				BoxHandleAdornment1.Parent = espFolder3
				BoxHandleAdornment1.Color = BrickColor.new("Institutional white")
				BoxHandleAdornment1.Transparency = transparency
				BoxHandleAdornment1.Size = Vector3.new(thicccness, 0.899999976, thicccness)
				BoxHandleAdornment1.Color3 = Color3.new(1, 1, 1)
				BoxHandleAdornment1.ZIndex = 10
				BoxHandleAdornment1.Adornee = v.Parent.LeftLowerLeg
				BoxHandleAdornment1.AlwaysOnTop = true
				BoxHandleAdornment1.SizeRelativeOffset = Vector3.new(0, -0.200000003, 0)

