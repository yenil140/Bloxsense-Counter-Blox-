plr=players.LocalPlayer

function round(n) 
	return math.floor(n + 0.5) 
end

local ThreadC = coroutine.create(function()
	while wait() do
		if script.DISTANCEESP.Value == false then
			script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUCOLOR.Value
		else
			script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUTOGGLEDCOLOR.Value
			pcall(function()
				for _,v in pairs(game.Players:GetChildren()) do
					if workspace:FindFirstChild(v.Name) then
						for _,o in pairs(workspace[v.Name].HumanoidRootPart:GetChildren()) do
							if o:IsA("BillboardGui") and o.Name=="distanceesp" then
								for _,x in pairs(o:GetChildren()) do
									pcall(function()
										local Color = script.Parent.Color.BackgroundColor3
										x.Text = round((game.Players.LocalPlayer.Character.Head.Position-game.Workspace[v.Name].Head.Position).Magnitude/2.5).." m"
										x.TextColor3 = Color	
									end)
								end
							end
						end
					end
				end
			end)
		end
	end
end)

function esp(v)
	for _,o in pairs(workspace[v.Name]:GetChildren()) do
		if o.Name=="HumanoidRootPart" then 
			local GUI = Instance.new('BillboardGui', o)
			local distanceesp = Instance.new('TextLabel', GUI)
			local constraint = Instance.new("UITextSizeConstraint", distanceesp)

			GUI.Adornee = o
			GUI.Size = UDim2.new(100, 0, 22, 0)
			GUI.StudsOffset = Vector3.new(0, -15.1, 0)
			GUI.Parent = o
			GUI.AlwaysOnTop = true
			GUI.Name = "distanceesp"

			distanceesp.BackgroundTransparency = 1
			distanceesp.Position = UDim2.new(0, 0, 0, 14)
			distanceesp.Size = UDim2.new(1, 0, 1, 0)
			distanceesp.Font = Enum.Font.SourceSans
			distanceesp.RichText = true
			distanceesp.TextScaled = true
			distanceesp.TextSize = 15
			distanceesp.TextStrokeColor3 = Color3.fromRGB(94, 94, 94)
			distanceesp.TextStrokeTransparency = 0.7
			distanceesp.TextXAlignment = Enum.TextXAlignment.Center
			distanceesp.TextYAlignment = Enum.TextYAlignment.Top

			constraint.MaxTextSize = 14
			constraint.MinTextSize = 14
			o.Parent.HumanoidRootPart.AncestryChanged:connect(function()
				GUI:Destroy()
			end)
		end
	end
end

local ThreadC1 = coroutine.create(function()
	script.DISTANCEESP.Changed:Connect(function()
		if script.DISTANCEESP.Value == true then
			for i,v in next, game:GetService'Players':GetChildren() do
				if v~=plr then
					if workspace:FindFirstChild(v.Name) then
						if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
							if script.Parent.Parent.Teammates.teammates.TEAMMATEESP.Value == true then
								esp(v)
							end
						else
							esp(v)
						end
					end
				end
			end
		else
			pcall(function()
				for _,v in pairs(game.Players:GetChildren()) do
					if workspace:FindFirstChild(v.Name) then
						for _,o in pairs(workspace[v.Name].HumanoidRootPart:GetChildren()) do
							if o:IsA("BillboardGui") and o.Name=="distanceesp" then
								o:Destroy()
							end
						end
					end
				end
			end)
		end	
	end)
end)

local ThreadC2 = coroutine.create(function()
	for i,v in pairs(game.Players:GetChildren()) do
		v.CharacterAdded:connect(function()
			if v~=plr then
				if script.DISTANCEESP.Value == true then
					v.Character:WaitForChild("HumanoidRootPart")
					if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
						if script.Parent.Parent.Teammates.teammates.TEAMMATEESP.Value == true then
							esp(v)
						end
					else
						esp(v)
					end
				end
			end
		end)
	end
end)

local ThreadC3 = coroutine.create(function()
	game.Players.PlayerAdded:connect(function(v)
		v.CharacterAdded:connect(function()
			if v~=plr then
				if script.DISTANCEESP.Value == true then
					v.Character:WaitForChild("HumanoidRootPart")
					if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
						if script.Parent.Parent.Teammates.teammates.TEAMMATEESP.Value == true then
							esp(v)
						end
					else
						esp(v)
					end
				end
			end
		end)
	end)
end)

coroutine.resume(ThreadC)
coroutine.resume(ThreadC1)
coroutine.resume(ThreadC2)
coroutine.resume(ThreadC3)

local button = script.Parent
local function onButtonActivated()
	if script.DISTANCEESP.Value == false then
		script.DISTANCEESP.Value = true
	else
		script.DISTANCEESP.Value = false
	end
end

button.Activated:Connect(onButtonActivated)
end))
