local button = script.Parent
local plrs = game:GetService("Players")
local plr = plrs.LocalPlayer
local val
function FOV(func)
	return coroutine.resume(coroutine.create(func))
end

FOV(function()
	script.Parent.Parent.Background.Percent.Changed:Connect(function()
		if script.FOV.Value == true then
			val = tonumber(script.Parent.Parent.Background.Percent.Value*1.2)
			getsenv(game.Players.LocalPlayer.PlayerGui.Client).fieldofview = val
			game.Workspace.CurrentCamera.FieldOfView = val
		end
	end)
	spawn(function()
		script.FOV.Changed:Connect(function()
			if script.FOV.Value == false then
				getsenv(game.Players.LocalPlayer.PlayerGui.Client).fieldofview = 80
				game.Workspace.CurrentCamera.FieldOfView = 80
			end
		end)
	end)
	while true do
		wait()
		pcall(function()
			if script.FOV.Value == false then
				script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUCOLOR.Value
			else
				script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUTOGGLEDCOLOR.Value
			end
		end)
	end
end)

local function onButtonActivated()
	if script.FOV.Value == false then
		script.FOV.Value = true			
	else
		script.FOV.Value = false
	end
end

button.Activated:Connect(onButtonActivated)
end))
