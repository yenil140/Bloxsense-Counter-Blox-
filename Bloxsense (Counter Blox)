table.insert(cors,sandbox(LocalScript1104,function()
local button = script.Parent
local toggled = false
local lighting = game.Lighting

function DeleteFX()
	local returnTable = {}
	for i,v in pairs (lighting:GetChildren()) do
		if v:IsA("ColorCorrectionEffect") then
			if v.Name == "Brightness" then
				v.Parent = nil		
			end	
		end
	end
end


Instance.new("ColorCorrectionEffect")
local ColorCorrection = game.Lighting.ColorCorrection
ColorCorrection.Name = "Brightness"
local button = script.Parent
local toggled = false
local lighting = game.Lighting
local toggled = false

function BRIGHTNESSFX(func)
	return coroutine.resume(coroutine.create(func))
end

BRIGHTNESSFX(function()
	while wait() do
		if script.BRIGHTNESSFX.Value == false then
			script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUCOLOR.Value
			DeleteFX()
		else
			ColorCorrection.Parent = game.Lighting
			script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUTOGGLEDCOLOR.Value
			if script.Parent.Parent.DropDown.Selection.Text == ("Nightmode") then
				ColorCorrection.Brightness = -0.1
				ColorCorrection.Contrast = 0
				ColorCorrection.Saturation = 0
				ColorCorrection.TintColor = Color3.fromRGB(197, 197, 197)
				game.Lighting.GlobalShadows = true
			end

			if  script.Parent.Parent.DropDown.Selection.Text == ("Fullbright") then
				ColorCorrection.Brightness = 0
				ColorCorrection.Contrast = 0
				ColorCorrection.Saturation = 0
				ColorCorrection.TintColor = Color3.fromRGB(255, 255, 255)
				game.Lighting.GlobalShadows = false
			end
			if  script.Parent.Parent.DropDown.Selection.Text == ("Zuhn") then
				ColorCorrection.Brightness = 0
				ColorCorrection.Contrast = 0.3
				ColorCorrection.Saturation = 0.3
				ColorCorrection.TintColor = Color3.fromRGB(255, 250, 228)
				game.Lighting.GlobalShadows = true
			end
		end	
	end
end)

local function onButtonActivated()
	if script.BRIGHTNESSFX.Value == false then
		script.BRIGHTNESSFX.Value = true
	else
		script.BRIGHTNESSFX.Value = false
	end
end

button.Activated:Connect(onButtonActivated)


end))
