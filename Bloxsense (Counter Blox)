function hitmarker()
	-- Instances:

	local Hitmarker = Instance.new("ImageLabel")

	--Properties:

	Hitmarker.Name = "Hitmarker"
	Hitmarker.Parent = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent
	Hitmarker.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Hitmarker.BackgroundTransparency = 1.000
	Hitmarker.Position = UDim2.new(0.47, 0, 0.433, 0)
	Hitmarker.Size = UDim2.new(0, 150, 0, 150)
	Hitmarker.Image = "rbxassetid://6397154447"
	Hitmarker.ScaleType = Enum.ScaleType.Fit

	-- Scripts:

	local function EXGYDG_fake_script() -- Hitmarker.LocalScript 
		local script = Instance.new('LocalScript', Hitmarker)

		TweenService = game:GetService("TweenService")
		MainBackground = script.Parent
		local fadeOutTween1 = TweenService:Create(MainBackground, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, false, 0), {ImageTransparency = 1})
		fadeOutTween1:Play()
		wait(0.4)
		script.Parent:Destroy()

	end
	coroutine.wrap(EXGYDG_fake_script)()

end

local function onButtonActivated()
	if script.HITMARKER.Value == false then
		script.HITMARKER.Value = true		
	else
		script.HITMARKER.Value = false
	end
end

