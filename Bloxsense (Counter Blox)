LocalScript119.Name = "gunmods"
LocalScript119.Parent = ImageButton117
table.insert(cors,sandbox(LocalScript119,function()
local button = script.Parent
local LocalPlayer = game.Players.LocalPlayer
local cbClient = getsenv(LocalPlayer.PlayerGui:WaitForChild("Client"))


local GUNMODTHREAD6544 = coroutine.create(function()
	script.GUNMODS.Changed:Connect(function()
		if script.GUNMODS.Value == false then
			cbClient.ammocount = 0
			cbClient.ammocount2 = 0
		end
	end)
	while true do
		wait()
		if script.GUNMODS.Value == false then
			script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUCOLOR.Value
		else
			script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUTOGGLEDCOLOR.Value
			if script.Parent.Parent.DropDown2.Selection.Text == "Infinite" then
					cbClient.ammocount = 99999 -- primary ammo
					cbClient.ammocount2 = 99999 -- secondary ammo			
			end	
		end
	end
end)

coroutine.resume(GUNMODTHREAD6544)

local function onButtonActivated()
	if script.GUNMODS.Value == false then
		script.GUNMODS.Value = true
	else
		script.GUNMODS.Value = false
	end
end

button.Activated:Connect(onButtonActivated)



end))
