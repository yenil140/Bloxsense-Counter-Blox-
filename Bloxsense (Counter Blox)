LocalScript894.Name = "FakelagOP"
LocalScript894.Parent = ImageButton892
table.insert(cors,sandbox(LocalScript894,function()
local button = script.Parent
local choking = false
local player = game.Players.LocalPlayer
local hVAL = 0

spawn(function()
	script.Parent.HealthVAL.Value = 0
	local me = player.Name
	while wait() do
		if player.Character then
			pcall(function()
				script.Parent.HealthVAL.Value = workspace[me].Humanoid.Health	
			end)
		end
	end
end)

function choketick()
	game:GetService("NetworkClient"):SetOutgoingKBPSLimit(1)
	choking = true	
	if script.Parent.Parent.Parent.Parent.Parent.Visual.Main.ColoredModelsTab.Fakelagchams.LocalMaterial.FAKELAGVISUAL.Value == true then
		if script.Parent.Parent.Parent.Parent.Parent.Visual.Main.EffectsTab.ThirdPerson.TP.Value == true then
			pcall(function()
				for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren())do 
					if v:IsA("BasePart") and v.Name~="HumanoidRootPart"then
						local flpart = Instance.new("Part")
						flpart.CFrame = v.CFrame
						flpart.Anchored = true
						flpart.CanCollide = false
						flpart.Material = Enum.Material.ForceField
						flpart.Color = script.Parent.Parent.Parent.Parent.Parent.Visual.Main.ColoredModelsTab.Fakelagchams.Color.BackgroundColor3
						flpart.Name = "FLCHAM"
						flpart.Transparency = 0
						flpart.Size = v.Size - Vector3.new(-0.1, -0.1, -0.1)
						flpart.Parent = game.Workspace
					end
				end
			end)	
		end
	end
end

function stopchoke()
	game:GetService("NetworkClient"):SetOutgoingKBPSLimit(9e9)
	choking = false
	for i,v in pairs(game.Workspace:GetChildren()) do
		if v.Name == "FLCHAM" then
			v:Destroy()
		end
	end
end

local button = script.Parent

function FAKELAG(func)
	return coroutine.resume(coroutine.create(func))
end

FAKELAG(function()
	spawn(function()
		game["Run Service"].Stepped:Connect(function()
			if choking == true then
				if hVAL < 70/255 then
					hVAL = hVAL + ((script.Parent.Parent.LIM.Percent.Value/7.14285714)/2)/255
				end
			else
				if hVAL > 1/255 then
					hVAL = hVAL - ((script.Parent.Parent.LIM.Percent.Value/7.14285714)/1)/255
				end				
			end	
		end)
	end)
	spawn(function()
		script.Parent.HealthVAL.Changed:Connect(function()
			stopchoke()
		end)
	end)
	local flloopmain = coroutine.create(function()
		while wait(1/16) do
			if script.FAKELAG.Value == true then
				if script.Parent.Parent.Amount.Selection.Text == "Fluctuate" then
					choketick()
					wait(((script.Parent.Parent.LIM.Percent.Value/7.14285714)/20) * math.random(1, script.Parent.Parent.LIM.Percent.Value)/50)
					stopchoke()
					wait(((script.Parent.Parent.LIM.Percent.Value/7.14285714)/20) * math.random(1, script.Parent.Parent.LIM.Percent.Value)/50)
				elseif script.Parent.Parent.Amount.Selection.Text == "Static" then
					choketick()
					wait(((script.Parent.Parent.LIM.Percent.Value/7.14285714)/20))
					stopchoke()
					wait(2/16)
				end
			end
		end		
	end)
	coroutine.resume(flloopmain)
	while true do
		wait()
		if script.FAKELAG.Value == false then
			script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUCOLOR.Value
			script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.tags.Fakelag.Visible = false
		else
			script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUTOGGLEDCOLOR.Value
			script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.tags.Fakelag.Visible = true
			script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.tags.Fakelag.TextColor3 = Color3.fromHSV(math.clamp(hVAL, 1/255, 70/255), 163/255, 255/255)
		end
	end
end)
	

local function onButtonActivated()
	if script.FAKELAG.Value == false then
		script.FAKELAG.Value = true		
	else
		script.FAKELAG.Value = false		
	end
end
button.Activated:Connect(onButtonActivated)

end))
