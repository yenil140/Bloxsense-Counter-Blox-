								esp(v)
							end
						end
					else
						if script.Parent.Parent.DropDown.Selection.Text == "Corner" then
							esp2(v)
						else
							esp(v)
						end
					end
				end
			end
		end)
	end	
end)

local ThreadB4 = coroutine.create(function()
	game.Players.PlayerAdded:connect(function(v)
		v.CharacterAdded:connect(function()
			if v~=plr then
				if script.BOXESP.Value == true then
					v.Character:WaitForChild("HumanoidRootPart")
					if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
						if script.Parent.Parent.Teammates.teammates.TEAMMATEESP.Value == true then
							if script.Parent.Parent.DropDown.Selection.Text == "Corner" then
								esp2(v)
							else
								esp(v)
							end
						end
					else
						if script.Parent.Parent.DropDown.Selection.Text == "Corner" then
							esp2(v)
						else
							esp(v)
						end
					end
				end
			end
		end)
	end)
end)

coroutine.resume(ThreadB1)
coroutine.resume(ThreadB2)
coroutine.resume(ThreadB3)
coroutine.resume(ThreadB4)

local button = script.Parent
local function onButtonActivated()
	if script.BOXESP.Value == false then
		script.BOXESP.Value = true
	else
		script.BOXESP.Value = false
	end
end

button.Activated:Connect(onButtonActivated)
end))
