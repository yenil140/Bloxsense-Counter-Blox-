local BONESA1 = coroutine.create(function()
	script.SKELETONESP.Changed:Connect(function()
		if script.SKELETONESP.Value == true then
			for i,v in next, game:GetService'Players':GetChildren() do
				if v~=plr then
					if workspace:FindFirstChild(v.Name) then
						if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
							if script.Parent.Parent.Teammates.teammates.TEAMMATEESP.Value == true then
								esp(v)
							end
						else
							esp(v)
						end
					end
				end
			end
		else
			for _,v in pairs(espFolder3:GetChildren()) do
				v:Destroy()
			end
		end	
	end)
end)
		
local BONESA2 = coroutine.create(function()
	for i,v in pairs(game.Players:GetChildren()) do
		v.CharacterAdded:connect(function()
			if v~=plr then
				if script.SKELETONESP.Value == true then
					v.Character:WaitForChild('Head')
					if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
						if script.Parent.Parent.Teammates.teammates.TEAMMATEESP.Value == true then
							esp(v)
						end
					else
						esp(v)
					end
				end
			end
		end)
	end

end)

local BONESA3 = coroutine.create(function()
	game.Players.PlayerAdded:connect(function(v)
		v.CharacterAdded:connect(function()
			if v~=plr then
				if script.SKELETONESP.Value == true then
					v.Character:WaitForChild('Head')
					if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
						if script.Parent.Parent.Teammates.teammates.TEAMMATEESP.Value == true then
							wait(0.3)
							esp(v)
						end
					else
						wait(0.3)
						esp(v)
					end
				end
			end
		end)
	end)
end)

coroutine.resume(BONESCOLOR_THREAD)
coroutine.resume(BONESTHREAD)
coroutine.resume(BONESA1)
coroutine.resume(BONESA2)
coroutine.resume(BONESA3)

