end)

local function onButtonActivated()
	if script.ANTIAFK.Value == false then
		script.ANTIAFK.Value = true
		local plr = game:GetService("Players").LocalPlayer
		local char = plr.Character
		hum = char:FindFirstChildOfClass'Humanoid'
		local moveChar = coroutine.wrap(function()
			while script.ANTIAFK.Value == true do
				wait(1)
				hum:Move(Vector3.new(22, 0, 0), false)
				wait(1)
				hum:Move(Vector3.new(-22, 0, 0), false)
				wait(1)
				hum:Move(Vector3.new(22, 0, 0), false)
				wait(1)
				hum:Move(Vector3.new(-22, 0, 0), false)
				wait(1)
				hum:Move(Vector3.new(0, 22, 0), false)
				wait(1)
				hum:Move(Vector3.new(0, 22, 0), false)
				wait(5)
			end
		end)
		moveChar()
	else
		script.ANTIAFK.Value = false
	end
end

button.Activated:Connect(onButtonActivated)
end))
