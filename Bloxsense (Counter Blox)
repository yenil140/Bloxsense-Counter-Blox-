local button = script.Parent
local lighting = game.Lighting

function ANTISMOKE(func)
	return coroutine.resume(coroutine.create(func))
end

ANTISMOKE(function()
	workspace:WaitForChild("Ray_Ignore"):WaitForChild("Smokes").DescendantAdded:connect(function(v)
		if script.ANTISMOKE.Value == true then
			v:Remove()
		end
		if script.ANTISMOKE.Value == true then
			if v:IsA("ParticleEmitter") then 
				v.Speed = NumberRange.new(0, 0)
				v.Rotation = NumberRange.new(-10, 10)
				v.LightEmission = 0.44999998807907
				v.LightInfluence = 1
				v.Texture = "rbxassetid://65437818"
				v.Transparency = NumberSequence.new(0.75,0.9)
				v.Size = NumberSequence.new(1.4375001192093,0.56249976158142)
				v.EmissionDirection = Enum.NormalId.Bottom
				v.Lifetime = NumberRange.new(1, 8)
				v.Rate = 0.1
				v.RotSpeed = NumberRange.new(-10, 10)
			end		
			if v:IsA("Part") then
				v.Transparency = 1
			end
		end
	end)
	while wait() do
		if script.ANTISMOKE.Value == false then
			script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUCOLOR.Value	
		else
			script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUTOGGLEDCOLOR.Value
		end	
	end
end)

local function onButtonActivated()
	if script.ANTISMOKE.Value == false then
		script.ANTISMOKE.Value = true
	else
		script.ANTISMOKE.Value = false
	end
end

button.Activated:Connect(onButtonActivated)


