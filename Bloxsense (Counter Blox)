		Legit.Icon.IMG.ImageColor3  = dimmed_color2
		Visuals.Icon.IMG.ImageColor3  = dimmed_color2
		Settings.Icon.IMG.ImageColor3  = dimmed_color2
		Skins.Icon.IMG.ImageColor3  = dimmed_color2
		User.Icon.IMG.ImageColor3 = dimmed_color2
		Config.Icon.IMG.ImageColor3  = dimmed_color2

