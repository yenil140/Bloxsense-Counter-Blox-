local plrs = game:GetService("Players")
local plr = plrs.LocalPlayer
local client = getsenv(plr.PlayerGui.Client)

local BacktrackFolder = Instance.new("Folder")
BacktrackFolder.Parent = game.Workspace
BacktrackFolder.Name = "BACKTRACK TICKS"

local runserv = game:GetService("RunService")
runserv:BindToRenderStep("Rainbow", 1000, function()
	local hue = tick() % 5 / 5
	color = Color3.fromHSV(hue, 1, 1)
end)

spawn(function()
	while true do
		wait()
		if script.BACKTRACK.Value == false then
			script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUCOLOR.Value
		else
			script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUTOGGLEDCOLOR.Value	
		end

	end
end)

spawn(function()
	if script.BACKTRACK.Value == true then	
		while script.BACKTRACK.Value == true do
			wait(1/math.clamp(tonumber(script.Parent.Parent.DropDown1.Selection.Text), 4, 64))
			if script.Parent.Parent.DropDown.Selection.Text == "Super low" then
				BTLEVEL = 100
			end
			if script.Parent.Parent.DropDown.Selection.Text == "Low" then
				BTLEVEL = 250
			end
			if script.Parent.Parent.DropDown.Selection.Text == "Medium" then
				BTLEVEL = 500
			end
			if script.Parent.Parent.DropDown.Selection.Text == "High" then
				BTLEVEL = 1000
			end
			if script.Parent.Parent.DropDown.Selection.Text == "Maximum" then
				BTLEVEL = 2000
			end
			for i,v in pairs(game.Players:GetPlayers()) do
