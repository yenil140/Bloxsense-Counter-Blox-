LocalScript1216.Parent = ImageButton1215
table.insert(cors,sandbox(LocalScript1216,function()
local players=game:GetService("Players")
local currentPlayer=nil
local lplayer=players.LocalPlayer
currentPlayer=nil
plr=players.LocalPlayer
local TOGGLED = false

local ThreadF = coroutine.create(function()
	while wait() do
		if script.NAMEESP.Value == false then
			script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUCOLOR.Value
		else
			script.Parent.BackgroundColor3 = script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.setup.MENUTOGGLEDCOLOR.Value
			pcall(function()
				for _,v in pairs(game.Players:GetChildren()) do
					if workspace:FindFirstChild(v.Name) then
						for _,o in pairs(workspace[v.Name].HumanoidRootPart:GetChildren()) do
							if o:IsA("BillboardGui") and o.Name=="nameesp" then
								for _,x in pairs(o:GetChildren()) do
									local Color = script.Parent.Color.BackgroundColor3
									x.TextColor3 = Color
								end
							end
						end
					end
				end
			end)
		end
	end
end)

function esp(v)
	for _,o in pairs(workspace[v.Name]:GetChildren()) do
		if o.Name=="HumanoidRootPart" then 
			local GUI2 = Instance.new('BillboardGui', o)
			local nameesp = Instance.new('TextLabel', GUI2)
			local constraint = Instance.new("UITextSizeConstraint", nameesp)

			GUI2.Adornee = o
			GUI2.Size = UDim2.new(100, 0, 22, 0)
			GUI2.StudsOffset = Vector3.new(0, 14.1, 0)
			GUI2.Parent = o
			GUI2.AlwaysOnTop = true
			GUI2.Name = "nameesp"

			nameesp.BackgroundTransparency = 1
			nameesp.Position = UDim2.new(0, 0, 0, -2)
			nameesp.Size = UDim2.new(1, 0, 1, 0)
			nameesp.Font = Enum.Font.SourceSans
			nameesp.TextScaled = true
			nameesp.TextSize = 15
			nameesp.RichText = true
			nameesp.Text = v.Name
			nameesp.TextStrokeColor3 = Color3.fromRGB(94, 94, 94)
			nameesp.TextStrokeTransparency = 0.7
			nameesp.TextXAlignment = Enum.TextXAlignment.Center
			nameesp.TextYAlignment = Enum.TextYAlignment.Bottom

			constraint.MaxTextSize = 14
			constraint.MinTextSize = 14
			o.Parent.HumanoidRootPart.AncestryChanged:connect(function()
				GUI2:Destroy()
			end)	
		end
	end
end

local ThreadF1 = coroutine.create(function()
	script.NAMEESP.Changed:Connect(function()
		if script.NAMEESP.Value == false then
			pcall(function()
				for _,v in pairs(game.Players:GetChildren()) do
					if workspace:FindFirstChild(v.Name) then
						for _,o in pairs(workspace[v.Name].HumanoidRootPart:GetChildren()) do
							if o:IsA("BillboardGui") and o.Name=="nameesp" then
								o:Destroy()
							end
						end
					end
				end
			end)
		else
			for i,v in next, game:GetService'Players':GetChildren() do
				if v~=plr then
					if workspace:FindFirstChild(v.Name) then
						if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
							if script.Parent.Parent.Teammates.teammates.TEAMMATEESP.Value == true then
								esp(v)
							end
						else
							esp(v)
						end
					end
				end
			end
		end	
	end)
end)

local ThreadF2 = coroutine.create(function()
	for i,v in pairs(game.Players:GetChildren()) do
		v.CharacterAdded:connect(function()
			if v~=plr then
				if script.NAMEESP.Value == true then
					v.Character:WaitForChild("HumanoidRootPart")
					if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
						if script.Parent.Parent.Teammates.teammates.TEAMMATEESP.Value == true then
							esp(v)
						end
					else
						esp(v)
					end
				end
			end
		end)
	end
end)

local ThreadF3 = coroutine.create(function()
	game.Players.PlayerAdded:connect(function(v)
		v.CharacterAdded:connect(function()
			if v~=plr then
				if script.NAMEESP.Value == true then
					v.Character:WaitForChild("HumanoidRootPart")
					if game.Players[v.Name].Status.Team.Value==plr.Status.Team.Value then
						if script.Parent.Parent.Teammates.teammates.TEAMMATEESP.Value == true then
							esp(v)
						end
					else
						esp(v)
					end
				end
			end
		end)
	end)	
end)

coroutine.resume(ThreadF)
coroutine.resume(ThreadF1)
coroutine.resume(ThreadF2)
coroutine.resume(ThreadF3)

local button = script.Parent
local function onButtonActivated()
	if script.NAMEESP.Value == false then
		script.NAMEESP.Value = true
	else
		script.NAMEESP.Value = false
	end
end

button.Activated:Connect(onButtonActivated)
end))
